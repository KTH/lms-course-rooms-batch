{
  "name": "lms-course-rooms-batch",
  "version": "1.0.0",
  "description": "Script to synchronize course rooms in Canvas",
  "main": "src/app.ts",
  "scripts": {
    "start": "node --enable-source-maps dist/app.js",
    "dev": "ts-node-dev --respawn src/app.ts | pino-pretty -e options,response -o '{msg}' --ignore options,pid,hostname,app,req_id --singleLine --translateTime 'HH:MM:ss.l'",
    "format": "prettier --write .",
    "test:compile": "tsc --noEmit",
    "test:lint": "eslint src/**.ts",
    "test:format": "prettier --check .",
    "test:unit": "jest",
    "test": "npm run test:compile && npm run test:unit && npm run test:lint && npm run test:format",
    "build": "rm -rf dist && tsc"
  },
  "author": "KTH Royal Institute of Technology",
  "license": "MIT",
  "devDependencies": {
    "@jest/globals": "^27.4.6",
    "@kth/eslint-config-kth": "^2.0.2",
    "@swc-node/jest": "^1.4.3",
    "@swc/core": "^1.2.136",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "eslint": "^7.32.0",
    "jest": "^27.0.0",
    "pino-pretty": "^4.5.0",
    "prettier": "^2.5.1",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@kth/canvas-api": "^4.1.0",
    "@kth/reqvars": "^2.0.1",
    "dotenv": "^8.2.0",
    "fast-csv": "^4.3.6",
    "got": "^11.8.1",
    "jszip": "^3.7.1",
    "ldapts": "^2.11.0",
    "memoizee": "^0.4.15",
    "pino": "^6.11.1",
    "skog": "^2.1.0"
  }
}
